<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
       "http://struts.apache.org/dtds/struts-config_1_3.dtd">
       <struts-config>
       <form-beans>
       <form-bean name="loginForm" type="com.naresh.struts.login.LoginForm" ></form-bean>
       </form-beans>
       <action-mappings>
       <action path="/login" name="loginForm" type="com.naresh.struts.login.LoginAction" scope="session">
       <forward name="success" path="/view/success.jsp"/>
       </action>
       
       </action-mappings>
       
       </struts-config>